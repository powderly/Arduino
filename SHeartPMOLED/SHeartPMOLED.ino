void setup()
{
  Wire.begin();	
  SeeedOled.init();  //initialze SEEED OLED display
  DDRB|=0x21;        //digital pin 8, LED glow indicates Film properly Connected .
  PORTB |= 0x21;

  SeeedOled.setInverseDisplay();          // Set Display to inverse mode
  SeeedOled.clearDisplay();               // clear the screen and set start position to top left corner
  
}

void loop()
{
  SeeedOled.drawBitmap(SHealthSplash,1024);   // 1024 = 128 Pixels * 64 Pixels / 8
  //SeeedOled.clearDisplay();               // clear the screen and set start position to top left corner
  delay(2000);
  SeeedOled.drawBitmap(SHeartScreen, 1024);   // 1024 = 128 Pixels * 64 Pixels / 8
  //SeeedOled.clearDisplay();               // clear the screen and set start position to top left corner
  delay(2000);
  SeeedOled.drawBitmap(Circle, 1024);   // 1024 = 128 Pixels * 64 Pixels / 8
  //SeeedOled.clearDisplay();               // clear the screen and set start position to top left corner
  delay(2000);
}

